      <Content Include="bin\Debug\net8.0-windows\runtimes\win-x64\native\assimp.dll" />
      <Content Include="bin\Debug\net8.0-windows\runtimes\win-x86\native\assimp.dll" />
      <Content Include="bin\Debug\net8.0-windows\SharpDX.D3DCompiler.dll" />
